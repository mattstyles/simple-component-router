{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "downlevelIteration": true,
    "emitDeclarationOnly": true,
    "outDir": "./lib",
    "lib": ["es2019", "DOM", "ES6", "DOM.Iterable", "ScriptHost"]
  },
  "exclude": ["node_modules", "**/*/lib", "**/*/dist"]
}
